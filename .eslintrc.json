{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "ignorePatterns": [
    "/node_modules",
    "/dist"
  ],
  "rules": {
    "no-unused-vars": "off"
  }
}